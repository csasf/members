<p id="notice"><%= notice %></p>

<div class='pageHeader'><h2>Performance Sets</h2> &emsp; <%= link_to 'Add New Performance Set', new_performance_set_path %></div>

<table>
  <thead>
    <tr>
      <th>Set</th>
    </tr>
  </thead>

  <tbody>
    <% @performance_sets.each do |performance_set| %>
      <tr>
        <td><%= link_to "#{Ensemble.find(performance_set.ensemble_id).name} from #{performance_set.start_date} to #{performance_set.end_date}", performance_set %></td>
      </tr>
    <% end %>
  </tbody>
</table>
