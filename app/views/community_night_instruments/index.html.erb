<h1>Community Night Instruments</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @community_night_instruments.each do |community_night_instrument| %>
      <tr>
        <td><%= link_to 'Show', community_night_instrument %></td>
